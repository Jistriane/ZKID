# Frontend Environment Variables

# Stellar Network (testnet or mainnet)
VITE_STELLAR_NETWORK=testnet

# Contract IDs (fill after deployment)
VITE_VERIFIER_CONTRACT_ID=
VITE_CREDENTIAL_REGISTRY_CONTRACT_ID=
VITE_COMPLIANCE_ORACLE_CONTRACT_ID=

# Horizon and RPC URLs
VITE_HORIZON_URL=https://horizon-testnet.stellar.org
VITE_RPC_URL=https://soroban-testnet.stellar.org

# Development Settings
VITE_ENABLE_DEBUG=false
VITE_MOCK_PROOFS=false

# ElizaOS Agent (optional - for AI compliance assistant)
VITE_ELIZA_API_URL=http://localhost:3000
